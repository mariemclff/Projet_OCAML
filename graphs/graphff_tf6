      rankdir=LR;
      node [shape = circle];
3 -> 5  [label = "2"];
3 -> 4  [label = "0"];
4 -> 5  [label = "1"];
2 -> 4  [label = "1"];
2 -> 3  [label = "2"];
1 -> 4  [label = "0"];
1 -> 3  [label = "0"];
0 -> 2  [label = "3"];
0 -> 1  [label = "0"];
 }
